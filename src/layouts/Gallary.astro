---
import SEO from "../components/SEO.astro";

const {title, description, cover} = Astro.props;
---

<html lang="en">
	<SEO 
		title={title}
		description={description}
		banner={cover}
	/>

	<body class="antialiased bg-slate-900">
		<div class="container mx-auto flex flex-col px-5 pt-36 py-0 items-center justify-center">
			<slot />
		</div>
	</body>
</html>
